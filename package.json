{
  "name": "webdggrid",
  "version": "1.0.0",
  "description": "Wasm package for DGGRID ",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "prebuild-": "build cpp",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps",
    "docs-show": "npx docsify serve ./docs",
    "docs-gen": "cp -u README.md docs/ && npx jsdoc2md {src/**/*.js} > docs/api.md",
    "docs": "npm run docs-gen && npm run docs-show",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "vitest --watch",
    "prettier": "prettier --config .prettierrc --write .",
    "test:run": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/am2222/webDggrid.git"
  },
  "keywords": [
    "DGGRID",
    "DGGS",
    "GIS"
  ],
  "author": "Majid Hojati",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/am2222/webDggrid/issues"
  },
  "homepage": "https://github.com/am2222/webDggrid#readme",
  "dependencies": {
    "http-server": "^14.1.1",
    "vtest": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.5.0",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~5.44",
    "@typescript-eslint/parser": "~5.44",
    "docsify-cli": "^4.4.4",
    "docsify-fulltextsearch": "^1.1.1",
    "eslint": "~8.28",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "glob": "^9.3.0",
    "jest": "^29.5.0",
    "jsdoc-to-markdown": "^8.0.0",
    "module-alias": "^2.2.2",
    "prettier": "~2.8",
    "rimraf": "~3.0",
    "ts-jest": "^29.0.5",
    "tsutils": "~3.21",
    "typescript": "^5.0.2",
    "vitest": "^0.29.8"
  }
}
